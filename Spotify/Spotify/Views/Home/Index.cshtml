@model SpotifyAPI.Web.Models.
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Spotify API</h1>
    <p class="lead">Connecting to Spotify and pulling it's Artists, Albums, and Genres</p>

</div>

<table class="table">
    <tr>
        <th>
            Artists
        </th>
    </tr>
    @foreach (var item in Model.Name)
    {
        <tr>
            <td>
                @
            </td>
        </tr>
    }



</table>